{
  "name": "@livepeer/explorer",
  "version": "1.0.0-alpha.6",
  "private": true,
  "main": "src/electron/main.js",
  "dependencies": {
    "@livepeer/apollo": "1.0.0-alpha.6",
    "@livepeer/graphql-sdk": "1.0.0-alpha.6",
    "apollo-cache-inmemory": "1.5.1",
    "apollo-client": "2.5.1",
    "apollo-link": "1.2.11",
    "apollo-link-error": "^1.1.1",
    "axios": "^0.18.0",
    "big.js": "^5.0.3",
    "bn.js": "^4.11.8",
    "cross-env": "^5.2.0",
    "email-validator": "^2.0.4",
    "final-form": "^4.2.1",
    "graphql": "^14.0.2",
    "graphql-tag": "^2.8.0",
    "qrcode-react": "^0.1.16",
    "query-string": "^6.4.2",
    "react": "^16.10.1",
    "react-apollo": "2.5.5",
    "react-dom": "^16.10.1",
    "react-dom-confetti": "^0.1.0",
    "react-feather": "^1.0.8",
    "react-final-form": "^4.1.0",
    "react-final-form-hooks": "^1.0.1",
    "react-final-form-html5-validation": "^1.0.0",
    "react-joyride": "^2.0.0-11",
    "react-redux": "7.0.2",
    "react-router-dom": "^5.0.0",
    "react-scripts": "^3.1.2",
    "react-tooltip": "^3.5.0",
    "react-transition-group": "^4.0.0",
    "recompose": "^0.30.0",
    "redux": "^4.0.1",
    "redux-saga": "^1.0.2",
    "redux-thunk": "^2.2.0",
    "rmwc": "1.6.2",
    "styled-components": "4.2.0",
    "unstated": "2.1.1",
    "url-search-params-polyfill": "5.1.0",
    "web3": "github:ethereum/web3.js"
  },
  "scripts": {
    "build": "npm run -s build:web",
    "build:desktop": "PUBLIC_URL=./ npm run -s build:web && rm -rf dist && npm run -s build:electron",
    "build:electron": "ELECTRON_BUILDER_ALLOW_UNRESOLVED_DEPENDENCIES=1 build",
    "build:web": "react-scripts build",
    "copy-docs": "rm -rf ../../docs/explorer && mkdir ../../docs/explorer && cp docs/index.html ../../docs/explorer && cp README.md ../../docs/explorer",
    "coverage": "react-scripts test --env=jsdom --coverage",
    "deploy": "poosh",
    "eject": "react-scripts eject",
    "postbuild": "cp build/index.html build/404.html",
    "serve": "serve -s build",
    "dev": "cross-env PORT=3001 react-scripts start",
    "test": "react-scripts test"
  },
  "build": {
    "appId": "org.livepeer.livepeer-protocol-explorer",
    "productName": "Livepeer Protocol Explorer",
    "directories": {
      "buildResources": "public"
    },
    "mac": {
      "category": "public.app-category.developer-tools"
    }
  },
  "devDependencies": {
    "documentation": "^10.0.0",
    "electron": "^5.0.1",
    "electron-builder": "20.4.1",
    "flow-bin": "0.97.0",
    "gh-pages": "^2.0.1",
    "poosh-cli": "^2.0.0",
    "poosh-plugin-s3": "^2.0.0",
    "serve": "^11.0.0",
    "uglify-es": "^3.2.0"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
