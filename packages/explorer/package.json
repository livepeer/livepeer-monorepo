{
  "name": "@livepeer/explorer",
  "version": "1.0.0-alpha.5",
  "private": true,
  "main": "src/electron/main.js",
  "dependencies": {
    "@livepeer/apollo": "^1.0.0-alpha.5",
    "@livepeer/graphql-sdk": "^1.0.0-alpha.5",
    "@livepeer/merkle-miner": "^1.0.0-alpha.5",
    "apollo-cache-inmemory": "1.1.10",
    "apollo-client": "2.2.6",
    "apollo-link": "1.2.1",
    "apollo-link-error": "^1.1.1",
    "axios": "^0.18.0",
    "big.js": "^5.0.3",
    "bn.js": "^4.11.8",
    "email-validator": "^2.0.4",
    "final-form": "^4.2.1",
    "graphql": "^14.0.2",
    "graphql-tag": "^2.8.0",
    "qrcode-react": "^0.1.16",
    "query-string": "^5.0.1",
    "react": "^16.8.3",
    "react-apollo": "2.0.4",
    "react-dom": "^16.8.3",
    "react-dom-confetti": "^0.0.10",
    "react-feather": "^1.0.8",
    "react-final-form": "^3.6.3",
    "react-final-form-hooks": "^1.0.1",
    "react-final-form-html5-validation": "^1.0.0",
    "react-joyride": "^2.0.0-11",
    "react-redux": "5.0.7",
    "react-router-dom": "^4.2.2",
    "react-router-redux": "^5.0.0-alpha.8",
    "react-scripts": "^2.1.8",
    "react-tooltip": "^3.5.0",
    "react-transition-group": "^2.2.1",
    "recompose": "^0.26.0",
    "redux": "^3.7.2",
    "redux-saga": "^0.16.0",
    "redux-thunk": "^2.2.0",
    "rmwc": "1.6.2",
    "styled-components": "3.2.1",
    "unstated": "1.1.0",
    "url-search-params-polyfill": "3.0.0",
    "web3": "github:ethereum/web3.js"
  },
  "scripts": {
    "build": "npm run -s build:web",
    "build:desktop": "PUBLIC_URL=./ npm run -s build:web && rm -rf dist && npm run -s build:electron",
    "build:electron": "ELECTRON_BUILDER_ALLOW_UNRESOLVED_DEPENDENCIES=1 build",
    "build:web": "react-scripts build",
    "copy-docs": "rm -rf ../../docs/explorer && mkdir ../../docs/explorer && cp docs/index.html ../../docs/explorer && cp README.md ../../docs/explorer",
    "coverage": "react-scripts test --env=jsdom --coverage",
    "deploy": "poosh",
    "eject": "react-scripts eject",
    "postbuild": "cp build/index.html build/404.html",
    "serve": "serve -s build",
    "start": "cross-env PORT=3001 react-scripts start",
    "test": "react-scripts test"
  },
  "build": {
    "appId": "org.livepeer.livepeer-protocol-explorer",
    "productName": "Livepeer Protocol Explorer",
    "directories": {
      "buildResources": "public"
    },
    "mac": {
      "category": "public.app-category.developer-tools"
    }
  },
  "devDependencies": {
    "documentation": "6.1.0",
    "electron": "1.8.3",
    "electron-builder": "20.4.1",
    "flow-bin": "0.67.1",
    "gh-pages": "^1.1.0",
    "poosh-cli": "^2.0.0",
    "poosh-plugin-s3": "^2.0.0",
    "serve": "^10.1.1",
    "uglify-es": "^3.2.0"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
