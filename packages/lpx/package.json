{
  "name": "@livepeer/lpx",
  "description": "A command line protocol explorer. It has a console mode that allows you to interactively use the sdk.",
  "version": "1.0.0-alpha.5",
  "license": "MIT",
  "bin": {
    "lpx": "bin/index.js"
  },
  "module": "src/index.js",
  "scripts": {
    "prebuild": "rimraf bin",
    "build": "babel src --out-dir bin",
    "cmd": "f() { npm -s run build && node ./bin/index $@; };f",
    "copy-docs": "echo 'No copy-docs script specified'",
    "prepack": "npm -s run build",
    "test": "ava bin/index.test.js",
    "start": "run-p \"build -- --watch\""
  },
  "dependencies": {
    "@livepeer/sdk": "^1.0.0-alpha.5",
    "as-table": "^1.0.31",
    "cli-spinner": "0.2.10",
    "cli-table": "^0.3.1",
    "commander": "2.20.0",
    "console.table": "0.10.0",
    "inquirer": "6.3.1",
    "pretty-error": "^2.1.1",
    "react-blessed": "0.5.0",
    "tty-table": "2.7.0",
    "vorpal": "^1.12.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.3",
    "@babel/core": "^7.4.3",
    "@babel/plugin-proposal-object-rest-spread": "^7.4.3",
    "@babel/plugin-transform-flow-strip-types": "^7.4.0",
    "@babel/plugin-transform-modules-commonjs": "^7.4.3",
    "@babel/plugin-transform-runtime": "^7.4.3",
    "@babel/preset-env": "^7.4.3",
    "@babel/preset-react": "^7.0.0",
    "ava": "1.4.1",
    "rimraf": "^2.6.3"
  },
  "homepage": "https://livepeer.org",
  "repository": {
    "type": "git",
    "url": "https://github.com/livepeer/livepeerjs.git"
  }
}
