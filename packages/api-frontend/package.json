{
  "name": "@livepeer/api-frontend",
  "version": "2.0.0-alpha.1",
  "private": true,
  "scripts": {
    "dev": "run-p dev:babel dev:next",
    "dev:babel": "babel --ignore node_modules --out-dir dist -x '.tsx' --watch .",
    "dev:next": "next --port=3008",
    "build": "run-p build:babel build:next",
    "build:babel": "babel --ignore node_modules --out-dir dist -x '.tsx' .",
    "build:next": "next build && next export",
    "start": "next start --port=3008",
    "now-build": "next build",
    "tsc": "tsc"
  },
  "devDependencies": {
    "next": "^9.0.6",
    "next-transpile-modules": "^2.3.1",
    "typescript": "^3.6.3"
  },
  "license": "ISC",
  "dependencies": {
    "@mdx-js/react": "^1.5.0",
    "react": "^16.10.1",
    "react-dom": "^16.10.1",
    "theme-ui": "^0.2.43"
  }
}
