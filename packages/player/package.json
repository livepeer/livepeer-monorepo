{
  "name": "@livepeer/player",
  "version": "1.0.0-alpha.3",
  "private": true,
  "dependencies": {
    "@livepeer/apollo": "^1.0.0-alpha.3",
    "@livepeer/graphql-sdk": "^1.0.0-alpha.3",
    "@livepeer/sdk": "^1.0.0-alpha.3",
    "apollo-cache-inmemory": "1.1.10",
    "apollo-client": "2.2.6",
    "apollo-link": "1.2.1",
    "graphql": "0.13.0",
    "graphql-tag": "2.8.0",
    "query-string": "^5.0.1",
    "react": "^16.2.0",
    "react-apollo": "^2.0.1",
    "react-dom": "^16.2.0",
    "react-feather": "^1.0.7",
    "react-redux": "5.0.7",
    "react-router-dom": "^4.2.2",
    "react-router-redux": "^5.0.0-alpha.8",
    "react-scripts": "1.1.1",
    "recompose": "^0.26.0",
    "redux": "^3.7.2",
    "redux-saga": "^0.16.0",
    "redux-thunk": "^2.2.0",
    "styled-components": "3.2.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "copy-docs": "echo 'No copy-docs script specified'",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "predeploy": "PUBLIC_URL=https://livepeer.github.io/livepeerjs/ npm run build",
    "deploy": "npx gh-pages -d build",
    "prebuild": "cp webpack.config.prod.js ../../node_modules/react-scripts/config",
    "postbuild": "cp build/index.html build/404.html && npx uglify-es build/static/js/main.*.js -c -m -o build/static/js/$(ls build/static/js | head -n 1)",
    "serve": "npx serve -s build"
  },
