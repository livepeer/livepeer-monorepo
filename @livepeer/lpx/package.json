{
  "name": "@livepeer/lpx",
  "version": "0.0.0",
  "license": "MIT",
  "bin": {
    "lpx": "bin/index.js"
  },
  "module": "src/index.js",
  "scripts": {
    "build": "rm -rf bin && npx babel src --out-dir bin --ignore spec.js,test.js",
    "cmd": "f() { npm -s run build && node ./bin/index $@; };f",
    "copy-docs": "echo 'No copy-docs script specified'",
    "bootstrap": "npm -s run build",
    "test": "npx ava"
  },
  "dependencies": {
    "@livepeer/sdk": "0.0.0",
    "as-table": "^1.0.31",
    "babel-runtime": "^6.26.0",
    "cli-spinner": "0.2.8",
    "cli-table": "^0.3.1",
    "commander": "2.15.0",
    "console.table": "0.10.0",
    "inquirer": "5.1.0",
    "pretty-error": "^2.1.1",
    "react-blessed": "0.2.1",
    "tty-table": "2.5.9",
    "vorpal": "^1.12.0"
  },
  "devDependencies": {
    "ava": "0.25.0",
    "babel-cli": "^6.26.0",
    "babel-plugin-transform-flow-strip-types": "^6.22.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-register": "^6.26.0"
  },
  "ava": {
    "require": [
      "babel-register"
    ],
    "babel": "inherit"
  },
  "homepage": "https://livepeer.org",
  "repository": {
    "type": "git",
    "url": "https://github.com/livepeer/livepeerjs.git"
  }
}
